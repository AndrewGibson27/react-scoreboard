{
  "name": "react-scoreboard",
  "version": "0.1.0",
  "description": "A live-updating scoreboard powered by React and a Google spreadsheet",
  "scripts": {
    "token": "node server/auth/generateToken.js",
    "start": "cross-env NODE_ENV=development node -r \"babel-register\" ./server",
    "start:prod": "cross-env NODE_ENV=production node ./build/server.js && npm run scrape:prod",
    "scrape": "cross-env NODE_ENV=development node -r \"babel-register\" ./server/scrape/scrape",
    "scrape:prod": "cross-env NODE_ENV=production node ./build/scrape.js",
    "build": "webpack -p --config ./tools/webpack.client.prod.js && webpack -p --config ./tools/webpack.server.prod.js && webpack -p --config ./tools/webpack.scrape.prod.js",
    "clean": "rm -rf build && rm -rf public/assets/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewgibson27/react-scoreboard"
  },
  "keywords": [
    "react",
    "scoreboard",
    "isomorphic",
    "universal",
    "redux",
    "react-router",
    "google spreadsheet",
    "redial",
    "styled components"
  ],
  "author": "AndrewGibson27",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewgibson27/react-scoreboard/issues"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.5.0",
    "body-parser": "^1.14.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "2.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "google-auth-library": "^0.10.0",
    "googleapis": "^19.0.0",
    "helmet": "^2.0.0",
    "history": "^4.0.0",
    "hpp": "^0.2.0",
    "morgan": "^1.6.1",
    "node-schedule": "^1.2.1",
    "opn": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^4.4.0",
    "react-router": "^3.0.5",
    "react-slick": "^0.14.11",
    "redial": "^0.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "slick-carousel": "^1.6.0",
    "styled-components": "^2.0.0-19",
    "throng": "^4.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.3.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-styled-components": "^1.1.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "style-loader": "^0.18.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "engines": {
    "node": "8.1.3"
  }
}
